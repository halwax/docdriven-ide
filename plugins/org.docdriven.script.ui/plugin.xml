<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.docdriven.script.ui.category"
            name="PDE Tools">
      </category>
      <command
            categoryId="org.docdriven.script.ui.category"
            defaultHandler="org.docdriven.script.ui.snapshot.TakeScreenShotHandler"
            id="org.docdriven.script.ui.snapshot.screenshot"
            name="Take a Screen Shot">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.docdriven.script.ui.snapshot.screenshot"
            contextId="org.eclipse.ui.contexts.dialogAndWindow"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M2+M3+3">
      </key>
   </extension>

</plugin>
